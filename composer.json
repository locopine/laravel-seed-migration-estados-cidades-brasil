{
    "name": "guiliredu/laravel-seed-migration-estados-cidades-brasil",
    "description": "Pacote com informações de todas cidades e estados brasileiros.",
    "keywords": [
        "guiliredu",
        "laravel-seed-migration-estados-cidades-brasil",
        "seed cidades brasileiras"
    ],
    "homepage": "https://github.com/guiliredu/laravel-seed-migration-estados-cidades-brasil",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Guilherme Redu",
            "email": "guiliredu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": ">=6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Guiliredu\\BrazilianCityMigrationSeed\\": "src",
            "Guiliredu\\BrazilianCityMigrationSeed\\Database\\Seeds\\": "database/seeds"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guiliredu\\BrazilianCityMigrationSeed\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guiliredu\\BrazilianCityMigrationSeed\\BrazilianCityMigrationSeedServiceProvider"
            ]
        }
    }
}
